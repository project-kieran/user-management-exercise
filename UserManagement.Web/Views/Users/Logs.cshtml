@model UserManagement.Web.Models.Users.LogEntryViewModel

<h2>Logs</h2>

<table>
    <thead>
        <tr>
            <th>User ID</th>
            <th>Timestamp</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var log in Model)
        {
        <tr>
            <td>@log.UserId</td>
            <td>@log.Timestamp.ToString("yyyy-MM-dd HH:mm:ss")</td>
            <td>@log.Action</td>
        </tr>
        }
    </tbody>
</table>

